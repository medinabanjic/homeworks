package ba.bitcamp.exercises.ass9_14;

public class TestConnectable {

	public static void main(String[] args) {
		
		char[] a1 = new char[]{'J','H','5','D','U','4','5','H','R','R'};
		char[] a2 = new char[]{'G','H','9','L','U','4','3','H','F','D'};
		
		Client c1 = new Client("ASUS 2. ",a1);
		Server s1 = new Server("MOJ SERVER", a2, 2);
		
		c1.connect(s1);
		c1.disconnect();
		c1.connect(s1);
		
	    System.out.println(c1);
	}

}
